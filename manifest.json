{
	"name": "TaskMaster",
	"manifest_version": 2,
	"version": "0.0",
	"author": "Jayvin Duong",
	"description": "Find Genome tasks with ease.",
	"content_security_policy": "script-src 'self' https://genome.klick.com; object-src 'self'",
	"browser_action": {
		"default_popup": "index.html"
	},
	"background": {
		"persistent": false,
		"scripts": [
			"scripts/taskmaster.min.js"
		]
	}
}